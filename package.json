{
	"name": "vscode-emacs-indent",
	"displayName": "vscode-emacs-indent",
	"description": "emacs like indent",
	"version": "0.0.1",
	"publisher": "sakapoko",
	"repository": {
		"type": "git",
		"url": "https://github.com/sakapoko/vscode-emacs-indent.git"
	},
	"engines": {
		"vscode": "^1.33.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:emacs-indent.reindentCurrentLine"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "emacs-indent.reindentCurrentLine",
				"title": "reindent current line"
			}
		],
		"keybindings": [
			{
				"key": "tab",
				"command": "emacs-indent.reindentCurrentLine",
				"when": "editorTextFocus"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"tslint": "^5.12.1",
		"@types/node": "^10.12.21",
		"@types/mocha": "^2.2.42"
	}
}
